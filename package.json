{
  "name": "node-ampq",
  "description": "Hands on practice with messaging queue",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf build",
    "build": "tsc --p tsconfig.build.json",
    "start": "ts-node src/index.ts",
    "dev": "nodemon -r tsconfig-paths/register src/index.ts | pino-pretty -c",
    "prepare": "husky",
    "prettier": "prettier --write .",
    "prettier:ci": "npx prettier --check .",
    "lint": "eslint \"**/*.ts\" --fix",
    "lint:ci": "eslint \"**/*.ts\"",
    "typecheck:ci": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "amqplib": "^0.10.3",
    "async-mutex": "^0.4.1",
    "axios": "^1.6.7",
    "axios-retry": "^4.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "express": "^4.18.3",
    "forever": "^4.0.3",
    "lodash": "^4.17.21",
    "p-retry": "4.6.2",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1",
    "prom-client": "^15.0.0",
    "redstone-protocol": "^1.0.5",
    "reflect-metadata": "^0.2.1",
    "tslib": "^2.6.2",
    "tslog": "^4.9.2",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@gearbox-protocol/eslint-config": "2.0.0-next.2",
    "@gearbox-protocol/prettier-config": "2.0.0-next.0",
    "@types/amqplib": "^0.10.5",
    "@types/commander": "^2.12.2",
    "@types/express": "^4.17.20",
    "@types/node": "^20.11.24",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "typescript-transform-paths": "^3.4.7",
    "vitest": "^1.3.1"
  },
  "prettier": "@gearbox-protocol/prettier-config",
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": "prettier --write"
  }
}
