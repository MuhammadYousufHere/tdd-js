{
  "name": "mern-practice",
  "version": "1.0.0",
  "description": "mern practice",
  "main": "src/main.js",
  "scripts": {
    "test": "jest",
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "start:dev": "nodemon src/main.js",
    "prebuild": "rimraf dist",
    "build": "babel --out-dir dist --ignore *.test.js src",
    "prepare": "husky install",
    "format": "yarn prettier --write",
    "prettier": "prettier --check **/*.js",
    "test:watch": "jest -i --watchAll",
    "coverage": "jest -i --coverage",
    "dkr:build": "docker build -t my-express:latest .",
    "dkr:run": "docker run -d -p 9876:9876 --name express-server my-express:latest"
  },
  "type": "module",
  "keywords": [
    "mern",
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": "Muhammad Yousuf <khanyousufmy80@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "7.23.4",
    "bcryptjs": "2.4.3",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "date-fns": "2.30.0",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "express-rate-limit": "7.1.4",
    "helmet": "7.1.0",
    "jsonwebtoken": "9.0.2",
    "mongodb": "6.3.0",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "passport": "0.6.0",
    "pm2": "5.3.0",
    "prom-client": "15.0.0",
    "validator": "13.11.0",
    "winston": "3.11.0"
  },
  "devDependencies": {
    "@babel/cli": "7.23.4",
    "@babel/core": "7.23.3",
    "@babel/plugin-transform-runtime": "7.23.4",
    "@babel/preset-env": "7.23.3",
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "eslint": "8.54.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-security": "1.7.1",
    "husky": "^8.0.0",
    "jest": "29.7.0",
    "lint-staged": "15.1.0",
    "prettier": "3.1.0",
    "rimraf": "5.0.5",
    "supertest": "6.3.3"
  }
}
